<div class="global-controls">
<label>Bypass <input class="bypass" type="checkbox" accesskey="b"></label>
<button class="reset" accesskey="r">Reset</button>
<label>Q: <input class="q" type="range" value="1.0" min="0.1" max="10.0" step="0.1" accesskey="q"></label>
</div>

<div class="bands">
{{#frequencyBands}}
<span class="band"><label>
<div class="frequency">{{frequency}}</div>
<input class="gain" type="range" orient="vertical" value="0" min="-30" max="30">
</label></span>
{{/frequencyBands}}
</div>
